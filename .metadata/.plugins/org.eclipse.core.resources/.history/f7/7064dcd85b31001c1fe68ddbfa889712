package project7;

public class FinalExam extends GradedActivity {
	
	private int numQuestions;
	private double pointsEach;
	private int numMissed;
	
	public FinalExam(int numQ, int numM) {
		numQuestions = numQ;
		numMissed = numM;
		setScore((numMissed/=numQuestions)*100);
	}
	public void setPointsEach(double p) { pointsEach = p; }
	public double getPointsEach() { return pointsEach; }
	public double getNumMissed() { return numMissed; }
	

}
